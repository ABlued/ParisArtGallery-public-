<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>JSP</display-name>
  
  <servlet>
	<servlet-name>LoginProcess</servlet-name>
	<servlet-class>LoginProcess.LoginProcess</servlet-class>
</servlet>

  <servlet-mapping>
  	<servlet-name>LoginProcess</servlet-name>
  	<url-pattern>/LoginProcess</url-pattern>
  </servlet-mapping>
  
    <servlet>
	<servlet-name>LogoutProcess</servlet-name>
	<servlet-class>LogoutProcess.LogoutProcess</servlet-class>
</servlet>

  <servlet-mapping>
  	<servlet-name>LogoutProcess</servlet-name>
  	<url-pattern>/LogoutProcess</url-pattern>
  </servlet-mapping>
</web-app>